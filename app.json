{
  "expo": {
    "name": "fmderecho",
    "slug": "fmderecho",
    "description": "Aplicación de la Franja Morada Derecho de la UNC. Accedé a toda la información de la Facultad y el Centro de Estudiantes a cualquier hora y desde cualquier lugar.",
    "privacy": "public",
    "owner": "alexistoledo",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.1.4",
    "githubUrl": "https://github.com/alexisjtoledo/fmderecho",
    "primaryColor": "#5B1F53",
    "backgroundColor": "#21201E",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/notification_icon.png"
    },
    "androidStatusBar":{
      "barStyle": "light-content",
      "backgroundColor": "#21201E",
      "hidden": false,
      "translucent": false
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5B1F53"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fmderecho.fmapp",
      "buildNumber": "3.1.4"
    },
    "android": {
      "package": "com.fmderecho.fmapp",
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#5B1F53",
        "backgroundImage": "./assets/images/android_icon_bg.png",
        "foregroundImage": "./assets/images/android_icon.png"
      },
      "permissions": [
        "CAMERA",
        "READ_INTERNAL_STORAGE"
      ]
    }
  }
}
